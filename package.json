    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.15.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "@types/vscode": "^1.85.0",
        "@vscode/vsce": "^2.15.0",
        "typescript": "^5.4.0"
    }
}
