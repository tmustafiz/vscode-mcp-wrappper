{
    "name": "vascode-mcp-wrapper",
    "displayName": "Internal DB Tools",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.99.0"
    },
    "enabledApiProposals": [
        "lmTools"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
